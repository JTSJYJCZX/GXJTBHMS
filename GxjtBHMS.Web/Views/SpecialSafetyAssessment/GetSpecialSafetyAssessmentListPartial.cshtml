@model GxjtBHMS.Web.ViewModels.SafetyAssessmentReportView.SafetyAssessmentReportSearchBaseView

@*<div id="message" style="color:@TempData[WebConstants.MessageColor]">@TempData[WebConstants.MessageKey]</div>*@
<div id="datasMessage"> </div>
<div class="table-responsive">
    <table class="table table-striped">
        <thead>
            <tr class="text-center">
                <td>报告名称</td>
                <td>报告生成时间</td>
                <td>下载</td>
                <td class="admintd">删除</td>
            </tr>
        </thead>

        @foreach (var item in Model.SafetyAssessmentReportViewModels)
        {
            @Html.Partial("GetSpecialSafetyAssessmentReportPartial", item);
        }
    </table>

    <script>
        jqPaginatorSetting('#paginationNav',@Model.PaginatorModel.TotalPages,@Model.PaginatorModel.CurrentPageIndex);
    </script>
    <script>
        $(function () {
            if (userName != "admin") {
                $("td.admintd").css("display", "none");
            }
        })
    </script>
</div>

@Html.AntiForgeryToken()

@Styles.Render("~/content/hotspotcss")
@Scripts.Render("~/bundles/hotspot")


 